<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">mock_exam_mockexam_form.mockexam_form_data_source</item>
		</item>
		<item name="label" xsi:type="string" translate="true">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button name="back" class="Mock\Exam\Block\Adminhtml\Mockexam\Edit\BackButton"/>
			<button name="delete" class="Mock\Exam\Block\Adminhtml\Mockexam\Edit\DeleteButton"/>
			<button name="save" class="Mock\Exam\Block\Adminhtml\Mockexam\Edit\SaveButton"/>
			<button name="save_and_continue" class="Mock\Exam\Block\Adminhtml\Mockexam\Edit\SaveAndContinueButton"/>
		</buttons>
		<namespace>mock_exam_mockexam_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>mock_exam_mockexam_form.mockexam_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="mockexam_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider name="mockexam_form_data_source" class="Mock\Exam\Model\Mockexam\DataProvider">
			<settings>
				<requestFieldName>mockexam_id</requestFieldName>
				<primaryFieldName>mockexam_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field name="question" formElement="textarea" sortOrder="10">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">mockexam</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">question</label>
				<dataScope>question</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="answer" formElement="textarea" sortOrder="20">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">mockexam</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">answer</label>
				<dataScope>answer</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="choice2" formElement="textarea" sortOrder="40">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">mockexam</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">choice2</label>
				<dataScope>choice2</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="choice3" formElement="textarea" sortOrder="50">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">mockexam</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">choice3</label>
				<dataScope>choice3</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="choice4" formElement="textarea" sortOrder="60">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">mockexam</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">choice4</label>
				<dataScope>choice4</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="choice5" formElement="textarea" sortOrder="70">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">mockexam</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">choice5</label>
				<dataScope>choice5</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="choice6" formElement="textarea" sortOrder="80">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">mockexam</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">choice6</label>
				<dataScope>choice6</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="type_question" formElement="textarea" sortOrder="90">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">mockexam</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">type_question</label>
				<dataScope>type_question</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
	</fieldset>
</form>
